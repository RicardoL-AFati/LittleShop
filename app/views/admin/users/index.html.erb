<body>
    <table>
        <thead>
            <th>Name</th>
            <th>Email</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Zip</th>
        </thead>
        <% @users.each do |user| %>
            <tbody>
                <td><%= user.name %></td>
                <td><%= user.address %></td>
                <td><%= user.city %></td>
                <td><%= user.state %></td>
                <td><%= user.zip %></td>
            </tbody>
        <% end %>
    </table>
</body>
</html>